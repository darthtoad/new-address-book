{{#partial "content"}}
    <h5>First Name:</h5>
    <p>{{foundEntry.first}}</p>
    <h5>Last Name:</h5>
    <p>{{foundEntry.last}}</p>
    <h5>Street:</h5>
    <p>{{foundEntry.address.street}}</p>
    <h5>City:</h5>
    <p>{{foundEntry.address.city}}</p>
    <h5>State:</h5>
    <p>{{foundEntry.address.state}}</p>
    <h5>Country:</h5>
    <p>{{foundEntry.address.country}}</p>
    <h5>Zip:</h5>
    <p>{{foundEntry.address.zip}}</p>
    <h5>Number:</h5>
    <p>{{foundEntry.number}}</p>
        {{#if published }}
            <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
        {{else}}
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
        {{/if}}

        <a href="/addressBook/{{foundEntry.addressId}}/edit">Edit this post</a><br>
        <a href="/addressBook/{{foundEntry.addressId}}/delete">DELETE this post (no undo)</a>
    </p>
{{/partial}}

{{> layout.hbs}}